.success_view {
  width: 750rpx;
  display: flex;
  flex-direction: column;
  align-items: center;

}

.success_view_image {
  width: 160rpx;
  height: 160rpx;
  margin-top: 200rpx;
}

.success_view_text {
  font-size: 35rpx;
  font-weight: bold;
  margin-top: 30rpx;
}

.success_view_tiem {
  width: 70%;
  margin-top: 20rpx;
  font-size: 26rpx;
  color: #a3a3a3;
  text-align: center;
  line-height: 50rpx;
}

.success_view_but {
  width: 100%;
  height: 80rpx;
  line-height: 80rpx;
  text-align: center;
  font-size: 26rpx;
  margin-top: 300rpx;
  justify-content: space-evenly;
  align-items: center;
}

.success_btn{
  width: 300rpx;
  height: 70rpx;
  line-height: 70rpx;
  font-size: 26rpx;
  background-color: #3963BC;
  color: #fff;
  margin: 0;
}